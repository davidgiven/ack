/****************************************
 * group 12 : Compare instructions      *
 ****************************************/

cmi $1==2       | source2 source2 |
			allocate(REG = {CONST2,0})
			"cmp %[1],%[2]"
			"beq 2f"
			"bgt 1f"
			"inc %[a]"
			"br 2f"
			"1:\tdec %[a]\n2:"
			setcc(%[a])
			erase(%[a])                             | %[a]  | |
ldc cmi zlt highw(1)==0 && loww(1)==0 && $2==4 | source2 source2 |
					| %[1] | zlt $3 |
ldc cmi zge highw(1)==0 && loww(1)==0 && $2==4 | source2 source2 |
					| %[1] | zge $3 |
cmi $1==4       | |     remove(all)
			"jsr pc,cmi4~"                  | r0    | |
#ifdef UNTESTED
