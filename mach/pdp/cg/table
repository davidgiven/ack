			remove(all)
			"cmp $$%(loww(1)%),%[2]"
			"jne $3"
			"cmp $$%(highw(1)%),%[1]"
			"jne $3"			|		| |
cms defined($1) | |     remove(all)
			move({CONST2,$1},r0)
			"jsr pc,cms~"
			erase(r0)                               | r0    | |
#ifdef UNTESTED
cms !defined($1)| source2 |
			remove(all)
