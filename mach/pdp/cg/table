			remove(all)
			move(%[1],r0)
			"jsr pc,cmu~"
			erase(r0)                               | r0    | |
#endif
cms $1==2       | |             |                       | cmi $1 |
ldc cms zeq $2==4 && loww(1)==0 && highw(1)==0 | source2 SCR_REG |
			remove(all)
			"bis %[1],%[2]"
			"jeq $3"			|		| |
ldc cms zne $2==4 && loww(1)==0 && highw(1)==0 | source2 SCR_REG |
			remove(all)
			"bis %[1],%[2]"
			"jne $3"			|		| |
ldc cms zeq $2==4	| source2 source2 |
			remove(all)
