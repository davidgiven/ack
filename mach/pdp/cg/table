			remove(all)
			move(%[1],r0)
			"jsr pc,cmi~"
			erase(r0)                               | r0    | |
#endif
cmf defined($1) | |     remove(ALL)
			move({CONST2,$1},r0)
			"jsr pc,cmf~"
			erase(r0)                               | r0    | |
#ifdef UNTESTED
cmf !defined($1)| source2 |
			remove(ALL)
			move(%[1],r0)
			"jsr pc,cmf~"
			erase(r0)                               | r0    | |
#endif
cmu $1==2       | |             |                       | cmp |
cmu $1==4       | |     remove(all)
			"jsr pc,cmu4~"                  | r0    | |
cmu defined($1) | |     remove(all)
			move({CONST2,$1},r0)
			"jsr pc,cmu~"                   | r0    | |
#ifdef UNTESTED
