			"jsr pc,cms~"
			erase(r0)                              | r0    | |
#endif
cmp             | source2 source2 |
			allocate(REG = {CONST2,0})
			"cmp %[1],%[2]"
			"beq 2f"
			"bhi 1f"
			"inc %[a]"
			"br 2f"
			"1:\tdec %[a]\n2:"
			setcc(%[a])
			erase(%[a])                             | %[a]  | |
tlt and $2==2	| source2 SCR_REG |
			test(%[1])
			"blt 1f"
			"clr %[2]\n1:"
			erase(%[2])				| %[2] | |
tlt ior $2==2	| source2 SCR_REG |
			test(%[1])
			"bge 1f"
			"bis $$1,%[2]\n1:"
			erase(%[2])				| %[2] | |
tlt             | source2 |
			allocate(REG={CONST2,0})
			test(%[1])
			"bge 1f"
