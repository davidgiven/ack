    kills ALL
    gen jsr {absolute4, ".mlu"}
			yields	dl1
#endif /* TBL68020 */

#if WORD_SIZE==2
pat dvu $1==2
with data2-sconsts data2
    uses DD_REG4 = {zero_const4,0}
    gen move %2,%a.1
	divu_w %1, %a.1			yields %a.1
#endif

pat dvu $1==4
#ifdef TBL68020
with data4-sconsts4 DD_REG4
    gen divu_l %1, %2	yields	%2
#else /* TBL68020 */
with STACK
    kills ALL
    gen jsr {absolute4, ".dvu"}
			yields	dl1
#endif /* TBL68020 */

#if WORD_SIZE==2
