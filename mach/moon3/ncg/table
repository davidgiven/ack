#if WORD_SIZE==2
pat cmi defined($1) && $1==2
with any2 DD_REG
    uses DD_REG = {zero_const, 0}
    gen cmp_w %1,%2
	beq {slabel,2f}
	bgt {slabel,1f}
	add_w {small_const, 1},%a
	bra {slabel,2f}
	1:
	sub_w {small_const, 1},%a
	2:
					yields %a

pat cmi defined($1) && $1==4
