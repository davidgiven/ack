with STACK
    kills ALL
    gen move {const, $1}, d0
	jsr {absolute4, ".set"}

pat set !defined($1)
with any_int STACK
    kills ALL
    gen move %1, d0
	jsr {absolute4, ".set"}
#else
pat set $1>4
with any_int STACK
    kills ALL
    gen move %1, d0
	move {const, $1}, d1
	jsr {absolute4, ".set"}

