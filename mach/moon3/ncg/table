#if TBL68020 && WORD_SIZE==4
with any4 D_REG4+DLOCAL+const4+ILOCAL+absolute4 STACK
#else
with any4 D_REG4+DLOCAL+const4+absolute4 STACK
#endif
    gen move %1,a0
	move %2,dl0
	jmp {absolute4, ".csb4"}

pat dch				leaving loi 4

