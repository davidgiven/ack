: '$Id$'
grep '^#' y.tab.h >tokendefs
ed - $1 <<'!Funky!Stuff!'
g/^#/d
1,$s/\([^ 	]*\)[ 	][ 	]*\(.*\)/	sy_p=lookup("\1",symkeyw,newsymbol);sy_p->sy_value.syv_keywno=\2;/
1i
#include "lookup.h"
.
.r tokendefs
a

enterkeyw() {
	register symbol *sy_p;

.
$a
}
.
w enterkeyw.c
q
!Funky!Stuff!
rm tokendefs
